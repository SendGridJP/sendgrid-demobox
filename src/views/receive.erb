<div class="container-fluid">
  <div class="row">

      <div class="alert alert-warning">
        Mailto: <a href="mailto:<%= @receive_address %>?subject=メール受信テスト&amp;body=メール受信テストの本文"><%= @receive_address %></a>
      </div>
      <table id="receive-table" class="table table-striped table-bordered table-hover table-condensed">
        <thead>
          <tr>
            <th>to</th>
            <th>from</th>
            <th>subject</th>
            <th>text</th>
            <th>charsets</th>
            <th>attachments</th>
